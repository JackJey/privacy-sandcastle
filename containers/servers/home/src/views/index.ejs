<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Privacy Sandcastle</title>
  <link rel="stylesheet" href="/css/global.css" />
  <link rel="icon" href="/img/spy.svg" />
  <script src="/js/main.js"></script>
</head>

<body>
  <h1>Privacy Sandcastle Home</h1>
  <ul>
    <% for (const { host, detail } of hosts ) { %>
    <li>
      <h2><%= detail %></h2>
      <iframe title="<%= host %>" src="<%= host %>"></iframe>
      <a target="_blank" href="<%= host %>"><%= host %></a>
    </li>
    <% } %>
  </ul>
</body>

</html>